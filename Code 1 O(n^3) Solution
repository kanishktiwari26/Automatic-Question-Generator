#include<bits/stdc++.h>
using namespace std;
int main() 
{
    int t;
    cin>>t;

      while(t--)
      {
         string s,k,ans;
         int n,max=0;
         cin>>s;
         n=s.length();
         for (int i = 0; i < n; i++)
         {
             for (int len = 1; len <= n - i; len++) 
             {
                  k=s.substr(i, len);
                  int l = 0,flag=0; 
                  int m=k.length();
                  int h = m - 1;

                  while (h > l) 
                  { 
                       if (k[l++] != k[h--]) 
                       { 
                          flag++;
                       } 
                  } 
             
                  if(flag==0)
                  {
                       if(m>max)
                       {
                            max=m;
                            ans=k;
                       }
    
                  }
                }
    
            }
            
        cout<<ans<<endl;
    
       }
return 0;
    
}


